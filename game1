a=input()
b=input()

if a=="가위":
   if b=="보":
      print("a가 이겼습니다.")
   elif b=="바위":
      print("b가 이겼습니다.")
   else:
      print("비겼습니다")

elif a=="바위":
   if b=="보":
      print("b가 이겼습니다.")
   elif b=="가위":
      print("a가 이겼습니다.")
   else:
       print("비겼습니다.")

elif a=="보":
   if b=="가위":
       print("b가 이겼습니다.")
   elif b=="바위":
      print("a가 이겼습니다.")
   else:
      print("비겼습니다.")
